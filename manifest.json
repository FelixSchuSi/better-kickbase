{
  "manifest_version": 2,
  "name": "better-kickbase",
  "version": "1.0",
  "description": "Make kickbase.com less annoying.",
  "icons": {
    "32": "favicon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://play.kickbase.com/*"
      ],
      "js": [
        "scripts/root.widget.ts"
      ],
      "css": [
        "styles/index.css",
        "styles/b-kb.css"
      ]
    }
  ]
}